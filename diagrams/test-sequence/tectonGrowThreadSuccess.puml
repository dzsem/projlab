@startuml tectonGrowThreadTest

title Tecton Grow Thread Success

actor "Tester" as tst
participant "t: Tecton" as t
participant "mt: MushroomThread" as mt
participant "l: Logger" as l

activate tst

tst -> l: printState(t)
activate l
tst <-- l
deactivate l

tst -> t: addConnection(mt)
activate t

tst <-- t
deactivate t

tst -> l: printState(t)
activate l
tst <-- l
deactivate l

deactivate tst

@enduml